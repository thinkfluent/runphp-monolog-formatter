{
    "name": "thinkfluent/runphp-monolog-formatter",
    "description": "Monolog JSON formatter for runphp / Google Cloud Run Toolkit",
    "keywords": ["google cloud run", "cloud run", "stackdriver", "monolog", "logging", "log"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tomwalder",
            "email": "tom@thinkfluent.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "ThinkFluent\\RunPHP\\Logging\\": "src/"
        }
    },
    "require": {
        "monolog/monolog": "^1|^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "lock": false,
        "sort-packages": true,
        "platform-check": false
    }
}
